{
    "configurations": [
        {
            "name": "Development server",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${config:python.defaultInterpreterPath}",
            "program": "${workspaceRoot}/website/manage.py",
            "args": [
                "runserver"
            ],
            "justMyCode": false
        },
        {
            "name": "Migrate",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${config:python.defaultInterpreterPath}",
            "program": "${workspaceRoot}/website/manage.py",
            "args": [
                "migrate"
            ],
            "justMyCode": false
        },
        {
            "name": "Collect static files",
            "type": "python",
            "request": "launch",
            "stopOnEntry": false,
            "python": "${config:python.defaultInterpreterPath}",
            "program": "${workspaceRoot}/website/manage.py",
            "args": [
                "collectstatic",
                "--noinput"
            ],
            "justMyCode": false
        }
    ]
}