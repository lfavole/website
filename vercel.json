{
    "builds": [
        {
            "src": "website/wsgi.py",
            "use": "@vercel/python"
        },
        {
            "src": "build.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "static"
            }
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "$1"
        },
        {
            "src": "/(.*)",
            "dest": "website/wsgi.py"
        }
    ]
}
