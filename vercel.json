{
    "installCommand": "pip install -r requirements.txt",
    "buildCommand": "python manage.py setup",
    "builds": [
        {
            "src": "website/wsgi.py",
            "use": "@vercel/python"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "website/wsgi.py"
        }
    ]
}
